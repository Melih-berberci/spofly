{
  "buildCommand": "",
  "outputDirectory": "public",
  "rewrites": [
    { "source": "/", "destination": "/demo.html" },
    { "source": "/(.*)", "destination": "/demo.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" }
      ]
    }
  ]
}
